{
    "name": "zeraton/calendize",
    "description": "A laravel-compatible CalDAV client",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Alexander Jank",
            "email": "alex@zeraton.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "sabre/vobject": "~4.1",
        "guzzlehttp/guzzle": "~6.0",
